{
  "name": "@nubase/core",
  "version": "0.1.18",
  "description": "Core schema and types for nubase",
  "keywords": [
    "schema",
    "types",
    "validation",
    "zod"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "test": "vitest run --passWithNoTests",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check . --write --unsafe",
    "prepublishOnly": "npm run build",
    "publish:latest": "npm publish --access public",
    "publish:dev": "npm publish --access public --tag dev",
    "watch:ts": "tsup --watch",
    "dev": "concurrently \"npm:watch:ts\""
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.1.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.17"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "zod": "^3.25.20"
  }
}
