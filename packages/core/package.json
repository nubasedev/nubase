{
  "name": "@nubase/core",
  "version": "0.1.0",
  "description": "Core schema and types for nubase",
  "keywords": [
    "schema",
    "types",
    "validation",
    "zod"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "test": "vitest run --passWithNoTests",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check . --write --unsafe",
    "prepublishOnly": "npm run build",
    "publish": "npm publish",
    "watch:ts": "tsup --watch",
    "dev": "concurrently \"npm:watch:ts\""
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.1.4"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "zod": "^3.25.20"
  }
}
