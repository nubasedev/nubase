{
  "name": "@nubase/frontend",
  "version": "0.1.29",
  "description": "React components and utilities for nubase",
  "keywords": [
    "react",
    "components",
    "forms",
    "table",
    "ui"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "scripts": {
    "build:ts": "tsup",
    "build:css": "tailwindcss -i ./src/styles.css -o ./dist/styles.css",
    "build:storybook": "storybook build",
    "build": "npm run build:ts && npm run build:css",
    "test": "vitest run --passWithNoTests",
    "check-types": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check . --write --unsafe",
    "prepublishOnly": "npm run build",
    "publish:latest": "npm publish --access public",
    "publish:dev": "npm publish --access public --tag dev",
    "watch:ts": "tsup --watch",
    "watch:css": "tailwindcss -i ./src/styles.css -o ./dist/styles.css --watch",
    "storybook": "storybook dev -p 6006",
    "dev": "concurrently \"npm:watch:ts\" \"npm:watch:css\" \"npm:storybook\""
  },
  "peerDependencies": {
    "react": "^19.2.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@storybook/addon-docs": "^10.1.11",
    "@storybook/addon-onboarding": "^10.1.11",
    "@storybook/addon-themes": "^10.1.11",
    "@storybook/react-vite": "^10.1.11",
    "@tailwindcss/cli": "^4.1.18",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^19.2.8",
    "@types/react-grid-layout": "^1.3.6",
    "@types/typo-js": "^1.2.2",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.0",
    "postcss": "^8.5.3",
    "storybook": "^10.1.11",
    "tailwindcss": "^4.1.18",
    "tsup": "^8.5.1",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.17"
  },
  "dependencies": {
    "@biomejs/biome": "^2.3.11",
    "@headlessui/react": "^2.2.9",
    "@monaco-editor/loader": "^1.7.0",
    "@nubase/core": "*",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/cli": "^4.1.18",
    "@tailwindcss/postcss": "^4.1.18",
    "@tanstack/react-form": "^1.27.7",
    "@tanstack/react-query": "^5.90.18",
    "@tanstack/react-router": "^1.150.0",
    "@tanstack/react-router-devtools": "^1.150.0",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "downshift": "^9.0.13",
    "lodash-es": "^4.17.22",
    "lucide-react": "^0.539.0",
    "monaco-editor": "^0.52.2",
    "monaco-spellchecker": "^0.6.0",
    "postcss": "^8.5.3",
    "react-dom": "^19.2.3",
    "react-grid-layout": "^2.2.2",
    "react-hotkeys-hook": "^5.2.3",
    "react-monaco-editor": "^0.59.0",
    "recharts": "^3.6.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "typo-js": "^1.3.1",
    "zod-to-json-schema": "^3.24.6"
  }
}
