{
  "name": "@nubase/frontend",
  "version": "0.1.2",
  "description": "React components and utilities for nubase",
  "keywords": [
    "react",
    "components",
    "forms",
    "table",
    "ui"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "scripts": {
    "build:ts": "tsup",
    "build:css": "tailwindcss -i ./src/styles.css -o ./dist/styles.css",
    "build:storybook": "storybook build",
    "build": "npm run build:ts && npm run build:css",
    "test": "vitest run --passWithNoTests",
    "check-types": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check . --write --unsafe",
    "prepublishOnly": "npm run build",
    "publish": "npm publish",
    "watch:ts": "tsup --watch",
    "watch:css": "tailwindcss -i ./src/styles.css -o ./dist/styles.css --watch",
    "storybook": "storybook dev -p 6006 --no-open",
    "dev": "concurrently \"npm:watch:ts\" \"npm:watch:css\""
  },
  "peerDependencies": {
    "react": "^19.1.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@storybook/addon-docs": "^9.1.0",
    "@storybook/addon-onboarding": "^9.1.0",
    "@storybook/addon-themes": "^9.1.0",
    "@storybook/react-vite": "^9.1.0",
    "@tailwindcss/cli": "^4.0.12",
    "@tailwindcss/postcss": "^4.0.12",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^19.0.12",
    "@types/typo-js": "^1.2.2",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.2.0",
    "postcss": "^8.5.3",
    "storybook": "^9.1.0",
    "tailwindcss": "^4.1.3",
    "tsup": "^8.0.0",
    "tw-animate-css": "^1.3.6",
    "typescript": "5.8.2",
    "vite": "^6.3.5",
    "vitest": "^3.1.4"
  },
  "dependencies": {
    "@biomejs/biome": "^2.2.0",
    "@headlessui/react": "^2.2.4",
    "@monaco-editor/loader": "^1.5.0",
    "@nubase/core": "*",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tailwindcss/cli": "^4.1.3",
    "@tailwindcss/postcss": "^4.1.3",
    "@tanstack/react-form": "^1.15.2",
    "@tanstack/react-query": "^5.66.0",
    "@tanstack/react-router": "^1.121.2",
    "@tanstack/react-router-devtools": "^1.121.12",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "downshift": "^9.0.10",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.539.0",
    "monaco-editor": "^0.52.2",
    "monaco-spellchecker": "^0.6.0",
    "postcss": "^8.5.3",
    "react-dom": "^19.1.1",
    "react-hotkeys-hook": "^5.1.0",
    "react-monaco-editor": "^0.59.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.0.12",
    "typo-js": "^1.2.5",
    "zod-to-json-schema": "^3.24.6"
  }
}
